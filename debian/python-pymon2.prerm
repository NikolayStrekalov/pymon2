#!/bin/bash
# prerm script for python-pymon2
#
# see: dh_installdeb(1)

set -e

case "$1" in
    remove|purge)
        service pymon2 stop || true
    ;;

    upgrade|deconfigure|failed-upgrade|abort-upgrade)
    ;;

    *)
        echo "prerm called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

#DEBHELPER#

exit 0

